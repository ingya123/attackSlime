<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="402.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="miniProject.LoginController">
   <children>
      <ImageView fitHeight="325.0" fitWidth="428.0" layoutX="-13.0" layoutY="-7.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/login.jpg" />
         </image>
      </ImageView>
      <Button layoutX="327.0" layoutY="146.0" mnemonicParsing="false" onAction="#login" prefHeight="53.0" prefWidth="74.0" text="Login" />
      <TextField fx:id="fxId" layoutX="182.0" layoutY="146.0" prefHeight="23.0" prefWidth="137.0" promptText="아이디 입력" />
      <PasswordField fx:id="fxPw" layoutX="182.0" layoutY="173.0" prefHeight="22.0" prefWidth="137.0" promptText="비밀번호 입력" />
      <Button layoutX="321.0" layoutY="265.0" mnemonicParsing="false" onAction="#exit" prefHeight="30.0" prefWidth="87.0" text="나가기" />
      <Button layoutX="108.0" layoutY="265.0" mnemonicParsing="false" onAction="#scoreBoard" prefHeight="30.0" prefWidth="87.0" text="점수판" />
      <Button layoutX="14.0" layoutY="265.0" mnemonicParsing="false" onAction="#newMembership" prefHeight="30.0" prefWidth="87.0" text="회원가입" />
      
   </children>
</AnchorPane>
